<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xmlns="http://java.sun.com/xml/ns/javaee"
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
	id="WebApp_ID" version="3.0">
	<display-name>Servlets</display-name>
	<servlet>
		<servlet-name>Login-Servlet</servlet-name>
		<servlet-class>com.example.servlet.LoginActionServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login-Generic-Servlet</servlet-name>
		<servlet-class>com.example.servlet.LoginActionGenericServlet</servlet-class>
	</servlet>
	<servlet>
		<servlet-name>Login-Http-Servlet</servlet-name>
		<servlet-class>com.example.servlet.LogicActionHttpServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>Login-Servlet</servlet-name>
		<url-pattern>/message-servlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login-Generic-Servlet</servlet-name>
		<url-pattern>/message-generic-servlet</url-pattern>
	</servlet-mapping>
	<servlet-mapping>
		<servlet-name>Login-Http-Servlet</servlet-name>
		<url-pattern>/message-http-servlet</url-pattern>
	</servlet-mapping>
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
</web-app>